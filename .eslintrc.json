{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "parser": "@babel/eslint-parser"
    },
    "rules": {
        "max-len": "off",
        "max-classes-per-file": "off",
        "no-restricted-syntax": "off",
        "no-use-before-define": "off",
        "class-methods-use-this": "off",
        "spellcheck/spell-checker": "off"
    }
}
